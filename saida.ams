addiu $sp, $sp, 4
sub   $t0, $zero, $t0 # inverte o sinal do número
addiu $sp, $sp, -4
sw    $t0, 0($sp)
# fim negativo
lw    $t0, 0($sp) # atribuindo à variável global global
la    $t1, global
sw    $t0, 0($t1)
lw    $s0, 0($sp)
addiu $sp, $sp, 4
la    $t1, global # lendo variável global global
lw    $s0, 0($t1)
addiu $sp, $sp, -4
sw    $s0, 0($sp)
# fim lendo variável global global
# inicio escreva
li    $v0, 1
lw    $a0, 0($sp)
syscall # fim escreva
lw    $s0, 0($sp)
addiu $sp, $sp, 4
li    $v0, 11 # novaLinha
li    $a0, 10
syscall       # fim novaLinha
addi  $sp, $sp, 4 # reajusta stack bloco
li    $v0, 10 # finalizando programa
syscall       # fim main

